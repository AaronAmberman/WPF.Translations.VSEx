<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib">
    <sys:String x:Key="About">относительно</sys:String>
    <sys:String x:Key="AboutAppMessage">Зарегистрированный переведенный тематический шаблон приложения WPF (LTT).</sys:String>
    <sys:String x:Key="Browse">Просматривать</sys:String>
    <sys:String x:Key="BrowseTitle">Найти файл</sys:String>
    <sys:String x:Key="Cancel">Отмена</sys:String>
    <sys:String x:Key="Chinese">中文</sys:String> <!-- Chinese Simplified -->
    <sys:String x:Key="Dark">темный</sys:String>
    <sys:String x:Key="English">English</sys:String>
    <sys:String x:Key="Error">Ошибка</sys:String>
    <sys:String x:Key="Errors">Ошибки</sys:String>
    <sys:String x:Key="French">français</sys:String>
    <sys:String x:Key="German">Deutsch</sys:String>
    <sys:String x:Key="Italian">italiano</sys:String>
    <sys:String x:Key="Japanese">日本語</sys:String>
    <sys:String x:Key="Korean">한국어</sys:String>
    <sys:String x:Key="Language">Язык</sys:String>
    <sys:String x:Key="Light">Свет</sys:String>
