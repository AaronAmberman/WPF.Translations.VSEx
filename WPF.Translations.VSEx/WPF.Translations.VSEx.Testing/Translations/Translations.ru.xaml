    <sys:String x:Key="No">Нет</sys:String>
    <sys:String x:Key="Norwegian">Norsk</sys:String>
    <sys:String x:Key="OK">ХОРОШО</sys:String>
    <sys:String x:Key="Portuguese">português</sys:String>
